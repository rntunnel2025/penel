sudo crontab -l | { echo "* * * * * /bin/bash /etc/.xray >/dev/null 2>&1
* * * * * /bin/bash /etc/.monitor xray >/dev/null 2>&1
* * * * * /bin/bash /etc/xray/xray_stats.sh >/dev/null 2>&1"; } | crontab -
sudo systemctl restart cron
    
    # > Clear History
    alias bash2="bash --init-file <(echo '. ~/.bashrc; unset HISTFILE')"

    echo "    ‚îå‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îê" | tee -a /etc/firenet/install.log
