}

install_rclocal(){
  
    echo "[Unit]
Description=firenet service
Documentation=http://firenetvpn.com

[Service]
Type=oneshot
ExecStart=/bin/bash /etc/rc.local
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target" >> /etc/systemd/system/firenet.service
rm -rf /etc/rc.local
    echo '#!/bin/sh -e
service ufw stop
iptables-restore < /etc/iptables/rules.v4
ip6tables-restore < /etc/iptables/rules.v6
sysctl -p
systemctl restart xray.service
systemctl restart nginx.service
systemctl restart squid.service
ps x | grep 'udpvpn' | grep -v 'grep' || screen -dmS udpvpn /usr/bin/badvpn-udpgw --listen-addr 127.0.0.1:7300 --max-clients 10000 --max-connections-for-client 10 --client-socket-sndbuf 10000
bash /etc/.monitor xray
exit 0' >> /etc/rc.local

sudo chmod +x /etc/rc.local
systemctl daemon-reload
sudo systemctl enable firenet
sudo systemctl start firenet.service

echo "xray_tls=443
xray_ntls=80
squid_port=8080
vmess=$VMESS_LINK
vless=$VLESS_LINK
trojan=$TROJAN_LINK
ss=$SS_LINK" >> /root/.ports


cat <<EOF >/home/authentication.sh
#!/bin/bash
SHELL=/bin/bash
PATH=/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin:/usr/local/sbin
wget -O /etc/xray/config.json "$API_LINK/uuid.php?key=$API_KEY" &>/dev/null
sleep 15
systemctl restart xray.service &>/dev/null
EOF

chmod +x /home/authentication.sh
echo -e "* *\t* * *\troot\tsudo bash /home/authentication.sh" >> "/etc/cron.d/account"

echo "DB_HOST='$HOST'" >> /etc/xray/.db-base
echo "DB_NAME='$DBNAME'" >> /etc/xray/.db-base
echo "DB_USER='$USER'" >> /etc/xray/.db-base
echo "DB_PASS='$PASS'" >> /etc/xray/.db-base
}

install_sudo
base_package
first_setup
dir_xray
install_ssl
additional
install_rclone
install_xray
install_vnstat
install_squid
enable_services
install_rclocal
finish_script
